{
  "serverTestPatterns": [
    "server/**/__tests__/**/*.js",
    "plugins/**/__tests__/**/*.js",
    "!plugins/**/server/**/__tests__/**/*"
  ],
  "buildSourcePatterns": [
    "LICENSE.txt",
    "NOTICE.txt",
    "package.json",
    "yarn.lock",
    "tsconfig.json",
    "index.js",
    "plugins/reporting/.phantom/*",
    "plugins/reporting/.chromium/*",
    "common/**/*",
    "plugins/**/*",
    "server/**/*",
    "typings/**/*",
    "webpackShims/*",
    "!**/README.md",
    "!__tests__",
    "!__tests__/**/*",
    "!**/__tests__",
    "!**/__tests__/**/*",
    "!plugins/**/*.test.js",
    "!plugins/**/__snapshots__",
    "!plugins/**/__snapshots__/*",
    "!plugins/**/__mocks__/*"
  ],
  "skipInstallDependencies": true
}
