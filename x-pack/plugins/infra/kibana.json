{
  "id": "infra",
  "version": "8.0.0",
  "kibanaVersion": "kibana",
  "requiredPlugins": [
    "share",
    "features",
    "usageCollection",
    "spaces",
    "embeddable",
    "data",
    "dataViews",
    "visTypeTimeseries",
    "alerting",
    "triggersActionsUi",
    "observability",
    "ruleRegistry",
    "unifiedSearch"
  ],
  "optionalPlugins": ["ml", "home", "embeddable", "osquery"],
  "server": true,
  "ui": true,
  "configPath": ["xpack", "infra"],
  "requiredBundles": [
    "unifiedSearch",
    "observability",
    "licenseManagement",
    "kibanaUtils",
    "kibanaReact",
    "ml",
    "embeddable"
  ],
  "owner": {
    "name": "Logs and Metrics UI",
    "githubTeam": "logs-metrics-ui"
  },
  "description": "This plugin visualizes data from Filebeat and Metricbeat, and integrates with other Observability solutions"
}
