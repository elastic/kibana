{
  "type": "plugin",
  "id": "@kbn/infra-plugin",
  "owner": "@elastic/infra-monitoring-ui",
  "description": "This plugin visualizes data from Filebeat and Metricbeat, and integrates with other Observability solutions",
  "plugin": {
    "id": "infra",
    "server": true,
    "browser": true,
    "configPath": [
      "xpack",
      "infra"
    ],
    "requiredPlugins": [
      "alerting",
      "cases",
      "charts",
      "data",
      "dataViews",
      "embeddable",
      "features",
      "lens",
      "observability",
      "ruleRegistry",
      "security",
      "share",
      "triggersActionsUi",
      "unifiedSearch",
      "usageCollection",
      "visTypeTimeseries",
    ],
    "optionalPlugins": [
      "spaces",
      "ml",
      "home",
      "embeddable",
      "osquery"
    ],
    "requiredBundles": [
      "unifiedSearch",
      "observability",
      "licenseManagement",
      "kibanaUtils",
      "kibanaReact",
      "ml",
      "embeddable",
      "controls"
    ]
  }
}
