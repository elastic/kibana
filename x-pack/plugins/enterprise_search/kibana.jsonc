  "plugin": {
    "id": "enterpriseSearch",
    "server": true,
    "browser": true,
    "configPath": [
      "enterpriseSearch"
    ],
    "requiredPlugins": [
      "features",
      "licensing",
      "data",
      "esUiShared",
    ],
    "optionalPlugins": [
      "customIntegrations",
      "globalSearch",
      "home",
      "indexManagement",
      "ml",
      "security",
      "spaces",
      "usageCollection",
      "guidedOnboarding",
      "console",
      "searchConnectors",
      "searchPlayground",
      "embeddable",
      "discover",
      "charts",
      "logsShared",
      "cloud",
      "lens",
      "share",
    ],
    "requiredBundles": [
      "kibanaReact"
    ]
  }
}
