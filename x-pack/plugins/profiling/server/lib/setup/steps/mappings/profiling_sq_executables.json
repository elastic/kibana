{
  "settings": {
    "index": {
      "refresh_interval": "10s"
    }
  },
  "mappings": {
    "_source": {
      "mode": "synthetic"
    },
    "properties": {
      "ecs.version": {
        "type": "keyword",
        "index": true
      },
      "Executable.file.id": {
        "type": "keyword",
        "index": false
      },
      "Time.created": {
        "type": "date",
        "index": true
      },
      "Symbolization.time.next": {
        "type": "date",
        "index": true
      },
      "Symbolization.retries": {
        "type": "short",
        "index": true
      }
    }
  }
}
