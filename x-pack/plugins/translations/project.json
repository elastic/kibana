{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/translations-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/server/**/*",
      "{projectRoot}/translations/ja-JP.json",
      "{projectRoot}/translations/zh-CN.json",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/translations",
  "implicitDependencies": []
}