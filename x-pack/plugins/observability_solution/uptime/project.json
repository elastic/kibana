{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/uptime-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/scripts/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/public/legacy_uptime/components/monitor/status_details/location_map/embeddables/low_poly_layer.json",
      "{projectRoot}/server/**/*",
      "{projectRoot}/server/legacy_uptime/lib/requests/__fixtures__/monitor_charts_mock.json",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/observability_solution/uptime"
}