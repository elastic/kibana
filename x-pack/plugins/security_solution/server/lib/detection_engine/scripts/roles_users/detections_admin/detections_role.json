{
  "elasticsearch": {
    "cluster": ["manage"],
    "indices": [
      {
        "names": [
          ".siem-signals-*",
          ".alerts-security*",
          ".preview.alerts-security*",
          ".internal.preview.alerts-security*",
          ".lists*",
          ".items*",
          "apm-*-transaction*",
          "traces-apm*",
          "auditbeat-*",
          "endgame-*",
          "filebeat-*",
          "logs-*",
          "packetbeat-*",
          "winlogbeat-*"
        ],
        "privileges": ["manage", "write", "read"]
      },
      {
        "names": ["metrics-endpoint.metadata_current_*", ".fleet-agents*", ".fleet-actions*"],
        "privileges": ["read"]
      }
    ]
  },
  "kibana": [
    {
      "feature": {
        "ml": ["all"],
        "siem": ["all", "read_alerts", "crud_alerts"],
        "securitySolutionCases": ["all"],
        "actions": ["read"],
        "builtInAlerts": ["all"],
        "dev_tools": ["all"]
      },
      "spaces": ["*"]
    }
  ]
}
