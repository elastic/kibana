{
  "elasticsearch": {
    "cluster": [],
    "indices": [
      {
        "names": [
          "apm-*-transaction*",
          "traces-apm*",
          "auditbeat-*",
          "endgame-*",
          "filebeat-*",
          "logs-*",
          "packetbeat-*",
          "winlogbeat-*",
          ".lists*",
          ".items*"
        ],
        "privileges": ["read", "write"]
      },
      {
        "names": [".alerts-security*", ".siem-signals-*"],
        "privileges": ["read", "write", "maintenance", "view_index_metadata"]
      },
      {
        "names": [
          "metrics-endpoint.metadata_current_*",
          ".fleet-agents*",
          ".fleet-actions*"
        ],
        "privileges": ["read"]
      }
    ]
  },
  "kibana": [
    {
      "feature": {
        "ml": ["read"],
        "siem": ["all", "read_alerts", "crud_alerts"],
        "securitySolutionCases": ["all"],
        "actions": ["read"],
        "builtInAlerts": ["all"]
      },
      "spaces": ["*"]
    }
  ]
}
