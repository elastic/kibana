{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/security-solution-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/scripts/**/*",
      "{projectRoot}/common/**/*.json",
      "{projectRoot}/server/**/*.json",
      "{projectRoot}/scripts/**/*.json",
      "{projectRoot}/public/**/*.json",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*",
      "!{projectRoot}/**/cypress/**",
      "!{projectRoot}/public/management/cypress.config.ts"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/security_solution"
}