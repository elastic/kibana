{
  "_xpack/migration/assistance": {
    "methods": [
      "GET"
    ],
    "patterns": [
      "_xpack/migration/assistance",
      "_xpack/migration/assistance/{index}"
    ],
    "documentation": "https://www.elastic.co/guide/en/elasticsearch/reference/master/migration-api-assistance.html"
  },
  "_xpack/migration/upgrade/{index}": {
    "url_params": {
      "wait_for_completion": ["true", "false"]
    },
    "methods": [
      "POST"
    ],
    "patterns": [
      "_xpack/migration/upgrade/{index}"
    ],
    "documentation": "https://www.elastic.co/guide/en/elasticsearch/reference/master/migration-api-upgrade.html"
  },
  "_xpack/migration/deprecations": {
    "methods": [
      "GET"
    ],
    "patterns": [
      "_xpack/migration/deprecations",
      "{index}/_xpack/migration/deprecations"
    ],
    "documentation": "https://www.elastic.co/guide/en/elasticsearch/reference/master/migration-api-deprecation.html"
  }
}
