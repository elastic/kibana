{
  "security.change_password": {
    "url_params": {
      "refresh": [
        "true",
        "false",
        "wait_for"
      ]
    },
    "methods": [
      "PUT",
      "POST"
    ],
    "patterns": [
      "_security/user/{username}/_password",
      "_security/user/_password"
    ],
    "documentation": "https://www.elastic.co/guide/en/elasticsearch/reference/current/security-api-change-password.html"
  }
}
