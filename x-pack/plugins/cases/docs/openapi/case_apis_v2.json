{
  "swagger" : "2.0",
  "info" : {
    "version" : "1",
    "title" : "Case APIs",
    "description": "Kibana APIs for cases",
    "termsOfService" : "",
    "license": {
      "name": "Elastic License 2.0",
      "url": "https://www.elastic.co/licensing/elastic-license"
    }
  },
  "host" : "localhost:5601",
  "basePath" : "/",
  "tags" : [ {
    "name" : "cases",
    "description": "Case APIs enable you to open and track issues."
  } ],
  "schemes" : [ "https" ],
  "security" : [
    {
      "basicAuth": [ ]
    },
    {
      "apiKey" : [ ]
    }
  ],
  "parameters": {
    "alert_id": {
      "in": "path",
      "name": "alertId",
      "description": "An identifier for the alert.",
      "required": true,
      "type": "string"
    },
    "case_id": {
      "in": "path",
      "name": "caseId",
      "description": "The identifier for the case. To retrieve case IDs, use the find cases API. All non-ASCII characters must be URL encoded.",
      "required": true,
      "type": "string"
    },
    "configuration_id": {
      "in": "path",
      "name": "configurationId",
      "description": "An identifier for the configuration.",
      "required": true,
      "type": "string"
    },
    "comment_id": {
      "in": "path",
      "name": "commentId",
      "description": "The identifier for the comment. To retrieve comment IDs, use the get case or find cases APIs.\n",
      "required": true,
      "type": "string"
    },
    "kbn_xsrf": {
      "in": "header",
      "type": "string",
      "name": "kbn-xsrf",
      "required": true
    },
    "owner": {
      "in": "query",
      "name": "owner",
      "description": "A filter to limit the response to a specific set of applications. If this parameter is omitted, the response contains information about all the cases that the user has access to read.\n",
      "type": "string"
    },
    "severity": {
      "in": "query",
      "name": "severity",
      "description": "The severity of the case.",
      "type": "string",
      "enum": [
        "critical",
        "high",
        "low",
        "medium"
      ]
    },
    "space_id": {
      "in": "path",
      "name": "spaceId",
      "description": "An identifier for the space.",
      "required": true,
      "type": "string"
    }
  },
  "paths" : {
    "/s/{spaceId}/api/cases/alerts/{alertId}": {
      "get": {
        "tags": [ "cases" ],
        "summary": "Returns the cases associated with a specific alert.",
        "operationId": "getCasesByAlert",
        "description": "You must have `read` privileges for the **Cases** feature in the **Management**, **Observability**, or **Security** section of the Kibana feature privileges, depending on the owner of the cases you're seeking.\n",
        "x-technical-preview": true,
        "parameters": [
          {
            "$ref": "#/parameters/alert_id"
          },
          {
            "$ref": "#/parameters/owner"
          },
          {
            "$ref": "#/parameters/space_id"
          }
        ],
        "responses": {
          "200": {
            "description": "Indicates a successful call."
          }
        },
        "x-doc" : {
          "tag" : "Case APIs"
        }
      }
    }
  },
  "securityDefinitions" : {
    "apiKey" : {
      "type" : "apiKey",
      "name" : "Authorization",
      "in" : "header"
    },
    "basicAuth": {
      "type": "basic"
    }
  },
  "definitions": {
  }
}