{
  "name": "Query with a list",
  "description": "Query with a list",
  "rule_id": "query-with-list",
  "risk_score": 1,
  "severity": "high",
  "type": "query",
  "query": "user.name: root or user.name: admin",
  "lists": [
    {
      "field": "source.ip",
      "values_operator": "excluded",
      "values_type": "exists"
    },
    {
      "field": "host.name",
      "values_operator": "included",
      "values_type": "match",
      "values": [
        {
          "name": "rock01"
        }
      ],
      "and": [
        {
          "field": "host.id",
          "values_operator": "included",
          "values_type": "match_all",
          "values": [
            {
              "name": "123456"
            }
          ]
        }
      ]
    }
  ]
}
