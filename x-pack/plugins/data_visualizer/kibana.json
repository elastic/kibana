{
  "id": "dataVisualizer",
  "version": "8.0.0",
  "kibanaVersion": "kibana",
  "server": true,
  "ui": true,
  "requiredPlugins": [
    "data",
    "usageCollection",
    "embeddable",
    "share",
    "discover",
    "fileUpload"
  ],
  "optionalPlugins": [
    "security",
    "maps",
    "home",
    "lens",
    "indexPatternFieldEditor"
  ],
  "requiredBundles": [
    "home",
    "kibanaReact",
    "maps",
    "esUiShared",
    "fieldFormats"
  ],
  "extraPublicDirs": [
    "common"
  ],
  "owner": {
    "name": "Machine Learning UI",
    "githubTeam": "ml-ui"
  },
  "description": "The Data Visualizer tools help you understand your data, by analyzing the metrics and fields in a log file or an existing Elasticsearch index."
}
