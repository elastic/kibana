{
  "elasticsearch": {
    "cluster": ["manage"],
    "indices": [
      {
        "names": [".items-*", ".lists-*", ".alerts-security.alerts-*", ".siem-signals-*"],
        "privileges": ["manage", "read", "write", "view_index_metadata", "maintenance"]
      },
      {
        "names": ["*"],
        "privileges": ["read"]
      },
      {
        "names": ["logs-osquery_manager*"],
        "privileges": ["read"]
      }
    ]
  },
  "kibana": [
    {
      "feature": {
        "siem": ["all"],
        "osquery": ["read", "run_saved_queries" ]
      },
      "spaces": ["*"]
    }
  ]
}

