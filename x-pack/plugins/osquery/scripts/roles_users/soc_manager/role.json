{
  "elasticsearch": {
    "cluster": ["manage"],
    "indices": [
      {
        "names": [".items-*", ".lists-*", ".alerts-security.alerts-*", ".siem-signals-*", "logs-*"],
        "privileges": ["manage", "read", "write", "view_index_metadata", "maintenance"]
      },
      {
        "names": ["*"],
        "privileges": ["read"]
      }
    ]
  },
  "kibana": [
    {
      "feature": {
        "discover": ["all"],
        "infrastructure": ["read"],
        "indexPatterns": ["all"],
        "observabilityCases": ["all"],
        "securitySolutionCases": ["all"],
        "ml": ["all"],
        "siem": ["all"],
        "savedObjectsManagement": ["all"],
        "osquery": ["all"],
        "visualize": ["read"],
        "actions": ["all"]
      },
      "spaces": ["*"]
    }
  ]
}

