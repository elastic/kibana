{
  "id": "ml",
  "version": "8.0.0",
  "kibanaVersion": "kibana",
  "configPath": [
    "xpack",
    "ml"
  ],
  "requiredPlugins": [
    "cloud",
    "data",
    "dataViews",
    "dataVisualizer",
    "discover",
    "embeddable",
    "features",
    "fieldFormats",
    "licensing",
    "share",
    "taskManager",
    "triggersActionsUi",
    "uiActions",
    "unifiedSearch"
  ],
  "optionalPlugins": [
    "alerting",
    "charts",
    "dashboard",
    "home",
    "licenseManagement",
    "management",
    "maps",
    "security",
    "spaces",
    "usageCollection"
  ],
  "server": true,
  "ui": true,
  "requiredBundles": [
    "charts",
    "dashboard",
    "esUiShared",
    "fieldFormats",
    "kibanaReact",
    "kibanaUtils",
    "maps",
    "savedObjects",
    "usageCollection",
    "unifiedSearch"
  ],
  "extraPublicDirs": [
    "common"
  ],
  "owner": {
    "name": "Machine Learning UI",
    "githubTeam": "ml-ui"
  },
  "description": "This plugin provides access to the machine learning features provided by Elastic."
}
