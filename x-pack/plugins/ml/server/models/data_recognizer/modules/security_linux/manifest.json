{
  "id": "security_linux",
  "title": "Security: Linux",
  "description": "Detect suspicious activity using ECS Linux events. Tested with Auditbeat and the Elastic agent.",
  "type": "linux data",
  "logoFile": "logo.json",
  "defaultIndexPattern": "auditbeat-*,logs-endpoint.events.*",
  "query": {
    "bool": {
      "should": [
        {
          "match": {
            "host.os.type": {
              "query": "linux",
              "operator": "OR"
            }
          }
        },
        {
          "match": {
            "host.os.family": {
              "query": "debian",
              "operator": "OR"
            }
          }
        },
        {
          "match": {
            "host.os.family": {
              "query": "redhat",
              "operator": "OR"
            }
          }
        },
        {
          "match": {
            "host.os.family": {
              "query": "suse",
              "operator": "OR"
            }
          }
        }
      ]
    }
  },
