{
  "groups": ["nginx"],
  "description": "Nginx Access Logs: Detect unusual visitor rate",
  "analysis_config" : {
    "bucket_span": "15m",
    "summary_count_field_name": "dc_remote_ips",
    "detectors": [
      {
        "detector_description": "nginx_access_visitor_rate",
        "function": "non_zero_count"
      }
    ],
    "influencers": []
  },
  "analysis_limits": {
    "model_memory_limit": "10mb"
  },
  "data_description": {
    "time_field": "@timestamp",
    "time_format": "epoch_ms"
  },
  "model_plot_config": {
    "enabled": true
  },
  "custom_settings": {
    "custom_urls": [
      {
        "url_name": "Raw Data",
        "url_value": "kibana#/discover/ML-Filebeat-Nginx-Access?_g=(time:(from:\u0027$earliest$\u0027,mode:absolute,to:\u0027$latest$\u0027))&_a=(columns:!(_source),filters:!(),index:\u0027INDEX_PATTERN_ID\u0027,interval:auto,query:(query_string:(analyze_wildcard:!t,query:\u0027*\u0027)),sort:!(\u0027@timestamp\u0027,desc))"
      }
    ]
  }
}
