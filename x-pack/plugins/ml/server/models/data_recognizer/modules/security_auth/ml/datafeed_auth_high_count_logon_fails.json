{
  "job_id": "auth_high_count_logon_fails",
  "indices": [
    "auditbeat-*",
    "logs-*",
    "filebeat-*",
    "winlogbeat-*"
  ],
  "max_empty_searches": 10,
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "event.category": "authentication"
          }
        },
        {
          "term": {
            "event.outcome": "failure"
          }
        }
      ]
    }
  }
}