{
  "id": "security_windows",
  "title": "Security: Windows",
  "description": "Detects suspicious activity using ECS Windows events. Tested with Winlogbeat and the Elastic agent.",
  "type": "windows data",
  "logoFile": "logo.json",
  "defaultIndexPattern": "winlogbeat-*,logs-endpoint.events.*",
  "query": {
    "bool": {
      "must": [
        {
          "bool": {
            "should": [
              {
                "match": {
                  "host.os.family": {
                    "query": "windows",
                    "operator": "OR"
                  }
                }
              },
              {
                "match": {
                  "host.os.type": {
                    "query": "windows",
                    "operator": "OR"
                  }
                }
              }
            ]
          }
        }
      ]
    }
  },
