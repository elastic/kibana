{
  "type": "plugin",
  "id": "@kbn/observability-plugin",
  "owner": "@elastic/actionable-observability",
  "plugin": {
    "id": "observability",
    "server": true,
    "browser": true,
    "configPath": ["xpack", "observability"],
    "requiredPlugins": [
      "alerting",
      "cases",
      "charts",
      "data",
      "dataViews",
      "embeddable",
      "exploratoryView",
      "features",
      "files",
      "guidedOnboarding",
      "inspector",
      "lens",
      "observabilityShared",
      "ruleRegistry",
      "triggersActionsUi",
      "security",
      "share",
      "spaces",
      "unifiedSearch",
      "visualizations"
    ],
    "optionalPlugins": ["discover", "home", "licensing", "usageCollection"],
    "requiredBundles": ["data", "kibanaReact", "kibanaUtils", "unifiedSearch"],
    "extraPublicDirs": ["common"]
  }
}
