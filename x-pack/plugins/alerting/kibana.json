{
  "id": "alerting",
  "server": true,
  "ui": true,
  "owner": {
    "name": "Kibana Alerting",
    "githubTeam": "kibana-alerting-services"
  },
  "kibanaVersion": "kibana",
  "configPath": ["xpack", "alerting"],
  "requiredPlugins": [
    "actions",
    "encryptedSavedObjects",
    "eventLog",
    "features",
    "kibanaUtils",
    "licensing",
    "taskManager"
