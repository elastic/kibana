{
  "id": "transform",
  "version": "kibana",
  "server": true,
  "ui": true,
  "requiredPlugins": [
    "data",
    "home",
    "licensing",
    "management",
    "features",
    "savedObjects",
    "share",
    "triggersActionsUi",
    "fieldFormats",
    "unifiedSearch"
  ],
  "optionalPlugins": [
    "security",
    "usageCollection",
    "spaces",
    "alerting"
  ],
  "configPath": ["xpack", "transform"],
  "requiredBundles": [
    "esUiShared",
    "discover",
    "kibanaUtils",
    "kibanaReact",
    "ml"
  ],
  "extraPublicDirs": [
    "common"
  ],
  "owner": {
    "name": "Machine Learning UI",
    "githubTeam": "ml-ui"
  },
  "description": "This plugin provides access to the transforms features provided by Elastic. Transforms enable you to convert existing Elasticsearch indices into summarized indices, which provide opportunities for new insights and analytics."
}
