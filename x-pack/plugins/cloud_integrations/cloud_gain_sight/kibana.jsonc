{
  "type": "plugin",
  "id": "@kbn/cloud-gainsight-plugin",
  "owner": "@elastic/kibana-core",
  "description": "When Kibana runs on Elastic Cloud, this plugin registers Gainsight as a shipper for telemetry.",
  "plugin": {
    "id": "cloudGainsight",
    "server": true,
    "browser": true,
    "configPath": [
      "xpack",
      "cloud_integrations",
      "gain_sight"
    ],
    "requiredPlugins": [
      "cloud"
    ]
  }
}
