{
  "type": "plugin",
  "id": "@kbn/cloud-chat-plugin",
  "owner": "@elastic/kibana-core",
  "description": "Chat available on Elastic Cloud deployments for quicker assistance.",
  "plugin": {
    "id": "cloudChat",
    "server": true,
    "browser": true,
    "configPath": [
      "xpack",
      "cloud_integrations",
      "chat"
    ],
    "requiredPlugins": [
      "cloud"
    ],
    "optionalPlugins": [
      "security"
    ]
  }
}
