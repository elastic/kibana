{
  "title": "Datasource",
  "type": "object",
  "description": "A package with a use case (eg prod_west). The use case ID must be unique. A datasource can have multiple streams.",
  "properties": {
    "name": {
      "type": "string",
      "example": "prod_west",
      "description": "Should be unique"
    },
    "package": {
      "$ref": "./package.v1.json"
    },
    "streams": {
      "type": "array",
      "items": {
        "$ref": "./stream.v1.json"
      }
    },
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "read_alias": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "package",
    "streams"
  ]
}
