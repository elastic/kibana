{
  "xpack.watcher.stats": {
    "url_params": {
      "metric": [
        "_all",
        "queued_watches",
        "current_watches",
        "pending_watches"
      ],
      "emit_stacktraces": "__flag__"
    },
    "methods": [
      "GET"
    ],
    "patterns": [
      "_watcher/stats",
      "_watcher/stats/{metrics}"
    ],
    "url_components": {
      "metrics": [
        "_all",
        "current_watches",
        "pending_watches",
        "queued_watches"
      ]
    },
    "documentation": "http://www.elastic.co/guide/en/elasticsearch/reference/current/watcher-api-stats.html"
  }
}
