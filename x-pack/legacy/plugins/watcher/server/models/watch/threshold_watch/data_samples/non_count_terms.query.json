{
  "size": 0,
  "query": {
    "bool": {
      "filter": {
        "range": {
          "@timestamp": {
            "from": "now-30d",
            "to": "now"
          }
        }
      }
    }
  },
  "aggs": {
    "bucketAgg": {
      "terms": {
        "field": "extension.raw",
        "size": 3,
        "order": {
          "metricAgg": "desc"
        }
      },
      "aggs": {
        "metricAgg": {
          "avg": {
            "field": "bytes"
          }
        }
      }
    }
  }
}
