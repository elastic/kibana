{
  "id": "rule-1",
  "description": "Detecting root and admin users",
  "index": ["auditbeat-*", "filebeat-*", "packetbeat-*", "winlogbeat-*"],
  "interval": "5m",
  "name": "Detect Root/Admin Users",
  "severity": 1,
  "type": "kql",
  "from": "now-6m",
  "to": "now",
  "kql": "user.name: root or user.name: admin"
}
