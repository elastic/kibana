{
  "name": "List - or",
  "description": "Query with a list that includes or. This rule should only produce signals when host.name exists and event.module is suricata and source.ip is 10.128.0.51",
  "rule_id": "query-with-list-or",
  "risk_score": 1,
  "severity": "high",
  "type": "query",
  "query": "host.name: *",
  "interval": "30s",
  "lists": [
    {
      "field": "event.module",
      "values_operator": "excluded",
      "values_type": "match",
      "values": [
        {
          "name": "suricata"
        }
      ]
    },
    {
      "field": "source.ip",
      "values_operator": "excluded",
      "values_type": "match",
      "values": [
        {
          "name": "10.128.0.51"
        }
      ]
    }
  ]
}
