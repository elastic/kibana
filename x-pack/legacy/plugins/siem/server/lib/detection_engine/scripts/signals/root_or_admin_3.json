{
  "id": "rule-3",
  "description": "Detecting root and admin users as an empty set",
  "index": ["auditbeat-*", "filebeat-*", "packetbeat-*", "winlogbeat-*"],
  "interval": "5m",
  "name": "Detect Root/Admin Users",
  "severity": "high",
  "type": "query",
  "from": "now-16y",
  "to": "now-15y",
  "query": "user.name: root or user.name: admin",
  "language": "kuery"
}
