{
  "rule_id": "a330464b-b7b2-4c26-a354-d52ae1102af0",
  "risk_score": 50,
  "description": "Linux: Netcat Network Connection",
  "immutable": true,
  "interval": "5m",
  "name": "Linux: Netcat Network Connection",
  "severity": "low",
  "type": "query",
  "from": "now-6m",
  "to": "now",
  "query": "process.name: (nc or ncat or netcat or netcat.openbsd or netcat.traditional)",
  "language": "kuery",
  "filters": [
    {
      "query": {
        "match": {
          "event.action": {
            "query": "socket_opened",
            "type": "phrase"
          }
        }
      }
    },
    {
      "query": {
        "match": {
          "destination.ip": {
            "query": "127.0.0.1",
            "type": "phrase"
          }
        }
      }
    },
    {
      "query": {
        "match": {
          "destination.ip": {
            "query": "::1",
            "type": "phrase"
          }
        }
      }
    }
  ],
  "enabled": false,
  "version": 1
}
