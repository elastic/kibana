{
  "rule_id": "24432eec-4da3-4965-a683-4efa3cf07248",
  "risk_score": 50,
  "description": "Command shell started by Svchost",
  "immutable": true,
  "interval": "5m",
  "name": "Command shell started by Svchost",
  "severity": "low",
  "type": "query",
  "from": "now-6m",
  "to": "now",
  "query": "process.parent.name:svchost.exe",
  "language": "kuery",
  "filters": [
    {
      "query": {
        "match": {
          "process.name": {
            "query": "cmd.exe",
            "type": "phrase"
          }
        }
      }
    },
    {
      "query": {
        "match": {
          "event.action": {
            "query": "Process Create (rule: ProcessCreate)",
            "type": "phrase"
          }
        }
      }
    }
  ],
  "enabled": false,
  "version": 1
}
