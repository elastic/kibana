{
  "rule_id": "52ee22ab-ed4b-4933-ab46-f7cb09cd5789",
  "risk_score": 50,
  "description": "Linux: Web Download",
  "immutable": true,
  "interval": "5m",
  "name": "Linux: Web Download",
  "severity": "low",
  "type": "query",
  "from": "now-6m",
  "to": "now",
  "query": "process.name: curl or wget",
  "language": "kuery",
  "filters": [
    {
      "meta": {
        "negate": false,
        "type": "phrase",
        "key": "event.action",
        "value": "socket_opened",
        "params": {
          "query": "socket_opened"
        },
        "disabled": false,
        "alias": null,
        "indexRefName": "kibanaSavedObjectMeta.searchSourceJSON.filter[0].meta.index"
      },
      "query": {
        "match": {
          "event.action": {
            "query": "socket_opened",
            "type": "phrase"
          }
        }
      },
      "$state": {
        "store": "appState"
      }
    }
  ],
  "enabled": false,
  "version": 1
}
