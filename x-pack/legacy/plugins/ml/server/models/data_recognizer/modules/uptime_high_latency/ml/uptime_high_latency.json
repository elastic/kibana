{
  "job_type": "anomaly_detector",
  "groups": ["uptime"],
  "description": "Uptime Heartbeat: Identify monitors that are exhibiting higher than normal latency.",
  "analysis_config": {
    "bucket_span": "1m",
    "detectors": [
      {
        "detector_description": "increased latency",
        "function": "high_mean",
        "field_name": "monitor.duration.us",
        "partition_field_name": "monitor.id"
      }
    ],
    "influencers": ["monitor.id", "monitor.name"]
  },
  "allow_lazy_open": true,
  "analysis_limits": {
    "model_memory_limit": "32mb"
  },
  "data_description": {
    "time_field": "@timestamp"
  },
  "custom_settings": {
    "created_by": "ml-module-uptime-heartbeat",
    "custom_urls": []
  }
}
