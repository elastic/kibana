{
    "job_id": "JOB_ID",
    "indices": [
      "INDEX_PATTERN_NAME"
    ],
    "query": {
        "bool": {
          "must": [
            {
              "term": {
                "event.action": "bound-socket"
                }
              }
          ],
          "must_not": [
            {
              "bool": {
                "should": [
                  {
                    "match": {
                      "process.name": "dnsmasq"
                    }
                  },
                  {
                    "match": {
                      "process.name": "docker-proxy"
                    }
                  },
                  {
                    "match": {
                      "process.name": "rpcinfo"
                    }
                  }
                ]
              }
            }
        ]
        }
      }
  }