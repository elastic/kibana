{
    "job_type": "anomaly_detector",
    "groups": [
      "siem"
    ],
    "description": "SIEM Auditbeat: Detect unusually rare downloads via curl and wget on Linux",
    "analysis_config": {
      "bucket_span": "15m",
      "detectors": [
        {
          "detector_description": "rare by \"process.title\"",
          "function": "rare",
          "by_field_name": "process.title"
        }
      ],
      "influencers": [
        "host.name",
        "destination.ip",
        "destination.port"
      ]
    },
    "analysis_limits": {
      "model_memory_limit": "256mb"
    },
    "data_description": {
      "time_field": "@timestamp",
      "time_format": "epoch_ms"
    }
  }
