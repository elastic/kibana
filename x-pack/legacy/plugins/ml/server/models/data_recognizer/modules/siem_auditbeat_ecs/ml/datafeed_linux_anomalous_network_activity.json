{
    "job_id": "JOB_ID",
    "indices": [
      "INDEX_PATTERN_NAME"
    ],
    "query": {
            "bool": {
              "filter": [
                {
                  "match": {
                    "event.action": {
                      "query": "connected-to"
                    }
                  }
                }
              ],
              "must_not": [
                {
                  "bool": {
                    "should": [
                      {"term": {"destination.ip": "127.0.0.1"}},
                      {"term": {"destination.ip": "127.0.0.53"}},
                      {"term": {"destination.ip": "::1"}}
                    ]
                  }
                }
              ]
            }
        }
    }
