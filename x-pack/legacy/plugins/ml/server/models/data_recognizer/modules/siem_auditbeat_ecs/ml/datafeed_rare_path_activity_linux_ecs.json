{
    "job_id": "JOB_ID",
    "indices": [
      "INDEX_PATTERN_NAME"
    ],
    "query": {
        "bool": {
            "filter": [
                {"match": {"event.action": { "query": "executed"}}}
            ],
            "must_not": [
                {
                    "bool": {
                        "should": [
                            {"match": {"user.name": "root"}},
                            {"match": {"user.name": "jenkins"}},
                            {"wildcard": {"process.cwd": {"value": "/hadoop*"}}},
                            {"wildcard": {"process.cwd": {"value": "*cache*"}}}
                        ]
                    }
                }
            ]
        }
    }
}
