{
    "job_id": "JOB_ID",
    "indices": [
      "INDEX_PATTERN_NAME"
    ],
    "query": {
      "bool": {
        "must": [
          {"match": {"event.action": "connected-to"}}
        ],
        "must_not": [
          {
            "bool": {
              "should": [
                {"match": {"destination.ip": "127.0.0.1"}},
                {"match": {"destination.ip": "::1"}},
                {"match": {"destination.ip": "::"}},
                {"match": {"user.name_map.uid": "jenkins"}}
              ]
            }
          }
        ]
      }
    }
  }