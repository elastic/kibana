{
  "job_id": "JOB_ID",
  "indexes": ["INDEX_PATTERN_NAME"],
  "aggregations": {
    "buckets": {
      "date_histogram": {
        "field": "@timestamp",
        "fixed_interval": "900000ms"
      },
      "aggregations": {
        "@timestamp": {
          "max": {
            "field": "@timestamp"
          }
        },
        "doc_count_per_minute": {
          "bucket_script": {
            "buckets_path": {
              "doc_count": "_count"
            },
            "script": "params.doc_count / 15"
          }
        }
      }
    }
  }
}
