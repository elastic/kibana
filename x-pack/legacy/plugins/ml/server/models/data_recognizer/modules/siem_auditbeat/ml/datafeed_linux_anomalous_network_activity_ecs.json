{
    "job_id": "JOB_ID",
    "indices": [
      "INDEX_PATTERN_NAME"
    ],
    "max_empty_searches": 10,
    "query": {
            "bool": {
              "filter": [
                {"term": {"event.action": "connected-to"}},
                {"term": {"agent.type": "auditbeat"}}
              ],
              "must_not": [
                {
                  "bool": {
                    "should": [
                      {"term": {"destination.ip": "127.0.0.1"}},
                      {"term": {"destination.ip": "127.0.0.53"}},
                      {"term": {"destination.ip": "::1"}}
                    ],
                    "minimum_should_match": 1
                  }
                }
              ]
            }
        }
    }
