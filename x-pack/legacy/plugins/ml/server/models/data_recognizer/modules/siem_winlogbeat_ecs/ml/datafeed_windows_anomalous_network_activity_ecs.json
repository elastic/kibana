{
    "job_id": "JOB_ID",
    "indices": [
      "INDEX_PATTERN_NAME"
    ],
    "query": {
        "bool": {
          "filter": [
            {"term": {"event.action": "Network connection detected (rule: NetworkConnect)"}}
          ],
          "must_not": [
            {
              "bool": {
                "should": [
                  {"term": {"destination.ip": "127.0.0.1"}},
                  {"term": {"destination.ip": "127.0.0.53"}},
                  {"term": {"destination.ip": "::1"}}
                ]
              }
            }
          ]
        }
    }
}
