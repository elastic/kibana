{
  "job_type": "anomaly_detector",
  "description": "SIEM Auditbeat: Detect unusual Linux processes which may indicate unauthorized services, malware or persistence mechanisms.",
  "groups": [
    "siem"
  ],
  "analysis_config": {
    "bucket_span": "15m",
    "detectors": [
      {
        "detector_description": "rare by \"process.name\"",
        "function": "rare",
        "by_field_name": "process.name"
      }
    ],
    "influencers": [
      "host.name",
      "process.name",
      "user.id"
    ]
  },
  "analysis_limits": {
    "model_memory_limit": "256mb"
  },
  "data_description": {
    "time_field": "@timestamp",
    "time_format": "epoch_ms"
  }
}