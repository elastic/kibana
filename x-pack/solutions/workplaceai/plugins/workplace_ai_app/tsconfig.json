    "@kbn/data-catalog-plugin",
    "@kbn/react-query",
    "@kbn/deeplinks-agent-builder",
    "@kbn/deeplinks-analytics",
    "@kbn/deeplinks-workflows",
    "@kbn/agent-builder-common",
    "@kbn/cloud",
    "@kbn/cloud-plugin",
    "@kbn/share-plugin",
    "@kbn/spaces-plugin",
