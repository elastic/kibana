    "@kbn/actions-plugin",
    "@kbn/features-plugin",
    "@kbn/logging",
    "@kbn/data-sources-registry-plugin",
    "@kbn/react-query",
    "@kbn/workplaceai-api-keys-components",
    "@kbn/workplaceai-api-keys-server",
    "@kbn/deeplinks-agent-builder",
    "@kbn/deeplinks-data-connectors",
