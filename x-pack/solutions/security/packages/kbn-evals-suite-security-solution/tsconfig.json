{
  "extends": "@kbn/tsconfig-base/tsconfig.json",
  "compilerOptions": {
    "outDir": "target/types",
    "types": [
      "jest",
      "node"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "**/*.ts",
    "**/*.json"
  ],
  "exclude": [
    "target/**/*"
  ],
  "kbn_references": [
    "@kbn/evals",
    "@kbn/tooling-log",
    "@kbn/core",
    "@kbn/scout-oblt",
    "@kbn/elastic-assistant-common",
    "@kbn/zod"
  ]
}

