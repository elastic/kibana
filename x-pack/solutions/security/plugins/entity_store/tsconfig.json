{
  "extends": "@kbn/tsconfig-base/tsconfig.json",
  "compilerOptions": {
    "outDir": "target/types"
  },
  "include": [
    "index.ts",
    "common/**/*.ts",
    "public/**/*.ts",
    "public/**/*.tsx",
    "server/**/*.ts",
    "../../typings/**/*"
  ],
  "exclude": ["target/**/*"],
  "kbn_references": [
    "@kbn/core",
    "@kbn/core-plugins-server",
    "@kbn/developer-examples-plugin",
    "@kbn/zod",
    "@kbn/logging",
    "@kbn/zod-helpers",
    "@kbn/task-manager-plugin",
    "@kbn/core-lifecycle-server",
    "@kbn/core-http-request-handler-context-server",
    "@kbn/core-http-server",
    "@kbn/config-schema",
    "@kbn/logging-mocks",
    "@kbn/spaces-plugin",
  ]
}
