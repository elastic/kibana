{
  "job_id": "JOB_ID",
  "indices": [
      "INDEX_PATTERN_NAME"
  ],
  "max_empty_searches": 10,
  "query": {
      "bool": {
          "filter": [
              {
                  "term": {
                      "event.code": "4648"
                  }
              }
          ],
          "must": [
              {
                  "bool": {
                      "should": [
                          {
                              "match": {
                                  "host.os.family": {
                                      "query": "windows",
                                      "operator": "OR"
                                  }
                              }
                          },
                          {
                              "match": {
                                  "host.os.type": {
                                      "query": "windows",
                                      "operator": "OR"
                                  }
                              }
                          }
                      ]
                  }
              }
          ],
          "must_not": {
              "terms": {
                  "process.name": [
                      "elastic-agent.exe", "elastic-agent", "metricbeat.exe", "metricbeat",
                      "filebeat.exe", "filebeat", "packetbeat.exe", "packetbeat",
                      "winlogbeat.exe", "winlogbeat"
                  ]
              }
          }
      }
  }
}