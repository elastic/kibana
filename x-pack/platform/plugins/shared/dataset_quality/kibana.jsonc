{
  "type": "plugin",
  "id": "@kbn/dataset-quality-plugin",
  "owner": [
    "@elastic/obs-ux-logs-team"
  ],
  "group": "platform",
  "visibility": "shared",
  "description": "This plugin introduces the concept of data set quality, where users can easily get an overview on the data sets they have.",
  "plugin": {
    "id": "datasetQuality",
    "browser": true,
    "server": true,
    "configPath": [
      "xpack",
      "datasetQuality"
    ],
    "requiredPlugins": [
      "controls",
      "data",
      "dataViewEditor",
      "dataViews",
      "embeddable",
      "fieldFormats",
      "fieldsMetadata",
      "fleet",
      "kibanaReact",
      "kibanaUtils",
      "lens",
      "share",
      "stackAlerts",
      "taskManager",
      "triggersActionsUi",
    ],
    "optionalPlugins": [
      "alerting",
      "telemetry",
      "triggersActionsUi",
      "usageCollection",
    ],
    "requiredBundles": [
      "discover",
      "unifiedHistogram",
    ],
    "extraPublicDirs": [
      "common"
    ]
  }
}
