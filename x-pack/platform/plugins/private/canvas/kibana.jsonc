{
  "type": "plugin",
  "id": "@kbn/canvas-plugin",
  "owner": [
    "@elastic/kibana-presentation"
  ],
  "group": "platform",
  "visibility": "private",
  "description": "Adds Canvas application to Kibana",
  "plugin": {
    "id": "canvas",
    "browser": true,
    "server": true,
    "configPath": [
      "xpack",
      "canvas"
    ],
    "requiredPlugins": [
      "charts",
      "data",
      "dataViews",
      "embeddable",
      "expressionError",
      "expressionImage",
      "expressionMetric",
      "expressionRepeatImage",
      "expressionRevealImage",
      "expressionShape",
      "expressions",
      "features",
      "inspector",
      "presentationUtil",
      "uiActions",
      "share",
      "contentManagement",
      "savedObjectsFinder"
    ],
    "optionalPlugins": [
      "home",
      "reporting",
      "spaces",
      "usageCollection"
    ],
    "requiredBundles": [
      "kibanaReact",
      "kibanaUtils",
      "lens",
      "maps",
      "visualizations",
      "fieldFormats"
    ]
  }
}