      "jest",
      "node",
      "react",
      "@kbn/ambient-ui-types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "target/**/*"
  ],
  "kbn_references": [
