  "name": "@kbn/foo-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/public/**/**/*",
      "{projectRoot}/server/**/**/*",
      "{projectRoot}/common/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
