    {
      "id": "system.process",
      "title": "System Process Metrics",
      "release": "beta",
      "type": "metrics",
      "streams": [
        {
          "input": "system/metrics",
          "vars": [
            {
              "default": "10s",
              "description": "Collection period",
              "name": "period",
              "type": "string"
            },
            {
              "default": [
                ".*"
              ],
              "description": "Processes to collect",
              "multi": true,
              "name": "processes",
              "type": "string"
            },
            {
              "default": true,
              "description": "These options allow you to filter out all processes that are not in the top N by CPU or memory, in order to reduce the number of documents created. If both the `by_cpu` and `by_memory` options are used, the union of the two sets is included. Set to false to disable this feature and include all processes\n",
              "name": "include_top_n.enabled",
              "type": "bool"
            },
            {
              "default": 5,
              "description": "How many processes to include from the top by CPU. The processes are sorted\n          by the `system.process.cpu.total.pct` field.\n",
              "name": "include_top_n.by_cpu",
              "type": "int"
            },
            {
              "default": 5,
              "description": "How many processes to include from the top by memory. The processes are sorted by the `system.process.memory.rss.bytes` field.\n",
              "name": "include_top_n.by_memory",
              "type": "int"
            },
            {
              "default": true,
              "description": "If false, cmdline of a process is not cached.",
              "name": "cmdline.cache.enabled",
              "type": "bool"
            },
            {
              "default": true,
              "description": "Enable collection of cgroup metrics from processes on Linux.",
              "name": "cgroups.enabled",
              "type": "bool"
            },
            {
              "description": "A list of regular expressions used to whitelist environment variables reported with the process metricset's events. Defaults to empty.\n",
              "multi": true,
              "name": "env.whitelist",
              "type": "string"
            },
            {
              "default": false,
              "description": "Include the cumulative CPU tick values with the process metrics. Defaults to false.\n",
              "name": "include_cpu_ticks",
              "type": "bool"
            }
          ]
        }
      ],
      "package": "system",
      "path": "process"
    },
