{
  "type": "index",
  "value": {
    "index": ".management-beats",
    "settings": {
      "index": {
        "codec": "best_compression",
        "number_of_shards": "1",
        "auto_expand_replicas": "0-1",
        "number_of_replicas": "0"
      }
    },
    "mappings": {
      "_doc": {
        "dynamic": "strict",
        "properties": {
          "type": {
