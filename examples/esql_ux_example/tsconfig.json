    "outDir": "target/types"
  },
  "include": [
    "public/**/*.ts",
    "public/**/*.tsx",
    "../../typings/**/*"
  ],
  "exclude": [
    "target/**/*"
  ],
  "kbn_references": [
    "@kbn/core",
    "@kbn/data-plugin",
    "@kbn/data-views-plugin",
    "@kbn/developer-examples-plugin",
    "@kbn/esql-editor",
    "@kbn/es-query",
    "@kbn/kibana-react-plugin",
    "@kbn/kibana-utils-plugin",
    "@kbn/ui-actions-plugin"
  ]
}
