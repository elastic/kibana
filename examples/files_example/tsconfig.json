    "@kbn/files-plugin",
    "@kbn/shared-ux-file-types",
    "@kbn/shared-ux-file-context",
    "@kbn/shared-ux-file-image",
    "@kbn/shared-ux-file-upload",
    "@kbn/shared-ux-file-picker",
    "@kbn/developer-examples-plugin",
  ]
}
