    "outDir": "target/types"
  },
  "include": [
    "index.ts",
    "common/**/*",
    "public/**/*.ts",
    "public/**/*.tsx",
    "server/**/*.ts",
    "../../typings/**/*"
  ],
  "exclude": [
    "target/**/*",
