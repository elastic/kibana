{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/core",
  "namedInputs": {
    "src": [
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/types/**/*",
      "{projectRoot}/test_helpers/**/*",
      "{projectRoot}/utils/**/*",
      "{projectRoot}/index.ts",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "src/core"
}