---

root: true
extends: '../../../../.eslintrc.js'

rules:
  block-scoped-var: off
  camelcase: off
  curly: off
  dot-location: off
  dot-notation: off
  eqeqeq: off
  guard-for-in: off
  indent: off
  max-len: off
  new-cap: off
  no-caller: off
  no-empty: off
  no-extend-native: off
  no-loop-func: off
  no-multi-str: off
  no-nested-ternary: off
  no-proto: off
  no-sequences: off
  no-use-before-define: off
  one-var: off
  quotes: off
  space-before-blocks: off
  space-in-parens: off
  space-infix-ops: off
  semi: off
  strict: off
  wrap-iife: off
  no-var: off
  prefer-const: off
