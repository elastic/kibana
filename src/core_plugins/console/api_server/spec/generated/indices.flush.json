{
  "indices.flush": {
    "url_params": {
      "force": "__flag__",
      "wait_if_ongoing": "__flag__",
      "ignore_unavailable": "__flag__",
      "allow_no_indices": "__flag__",
      "expand_wildcards": [
        "open",
        "closed",
        "none",
        "all"
      ]
    },
    "methods": [
      "POST",
      "GET"
    ],
    "patterns": [
      "_flush",
      "{indices}/_flush"
    ],
    "documentation": "http://www.elastic.co/guide/en/elasticsearch/reference/master/indices-flush.html"
  }
}
