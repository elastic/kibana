{
  "type": "plugin",
  "id": "@kbn/telemetry-plugin",
  "owner": "@elastic/kibana-core",
  "plugin": {
    "id": "telemetry",
    "server": true,
    "browser": true,
    "enabledOnAnonymousPages": true,
    "requiredPlugins": [
      "telemetryCollectionManager",
      "usageCollection",
      "screenshotMode"
    ],
    "optionalPlugins": [
      "home",
      "security"
    ],
    "requiredBundles": [
      "kibanaUtils",
      "kibanaReact"
    ],
    "extraPublicDirs": [
      "common/constants"
    ]
  }
}
