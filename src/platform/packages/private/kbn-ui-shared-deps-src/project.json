{
  "_preventUpdate": true,
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/ui-shared-deps-src",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.js",
      "!{projectRoot}/src/entry.js",
      "!{projectRoot}/**/*.config.js",
      "!{projectRoot}/target/**/*"
    ]
  },
  "implicitDependencies": [
    "@kbn/ui-shared-deps-npm"
  ],
  "targets": {
    "typecheck_fast": {
      "command": "yarn tsc --build {projectRoot}/tsconfig.type_check.json --pretty",
      "options": {
        "env": {
          "NODE_OPTIONS": "--max-old-space-size=8096"
        }
      }
    },
    "build": {
      "outputs": [
        "{workspaceRoot}/bazel-bin/src/platform/packages/private/kbn-ui-shared-deps-src"
      ]
    },
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    },
    "lint": {}
  },
  "projectType": "library",
  "sourceRoot": "src/platform/packages/private/kbn-ui-shared-deps-src"
}
