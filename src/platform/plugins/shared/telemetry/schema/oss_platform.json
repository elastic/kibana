                },
                "max": {
                  "type": "long"
                },
                "avg": {
                  "type": "float"
                }
              }
            },
            "scriptedFieldLineCount": {
              "properties": {
                "min": {
                  "type": "long"
                },
                "max": {
                  "type": "long"
                },
                "avg": {
                  "type": "float"
                }
              }
            },
            "runtimeFieldLineCount": {
              "properties": {
                "min": {
                  "type": "long"
                },
                "max": {
