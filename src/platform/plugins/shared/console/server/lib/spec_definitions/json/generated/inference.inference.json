{
  "inference.inference": {
    "url_params": {
      "error_trace": "__flag__",
      "filter_path": [],
      "human": "__flag__",
      "pretty": "__flag__",
      "timeout": [
        "30s",
        "-1",
        "0"
      ]
    },
    "url_components": {
      "task_type": [
        "sparse_embedding",
        "text_embedding",
        "rerank",
        "completion",
        "chat_completion"
      ]
    },
    "methods": [
      "POST"
    ],
    "patterns": [
      "_inference/{inference_id}",
      "_inference/{task_type}/{inference_id}"
    ],
    "documentation": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-inference-inference",
    "availability": {
      "stack": true,
      "serverless": true
    }
