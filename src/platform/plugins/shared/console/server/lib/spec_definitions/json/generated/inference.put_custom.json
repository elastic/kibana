{
  "inference.put_custom": {
    "url_params": {
      "error_trace": "__flag__",
      "filter_path": [],
      "human": "__flag__",
      "pretty": "__flag__"
    },
    "url_components": {
      "task_type": [
        "text_embedding",
        "sparse_embedding",
        "rerank",
        "completion"
      ]
    },
    "methods": [
      "PUT"
    ],
    "patterns": [
      "_inference/{task_type}/{custom_inference_id}"
    ],
    "documentation": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-inference-put-custom",
    "availability": {
      "stack": true,
      "serverless": true
    }
  }
}
