{
  "indices.data_streams_stats": {
    "url_params": {
      "expand_wildcards": [
        "open",
        "closed",
        "hidden",
        "none",
        "all"
      ],
      "forbid_closed_indices": "__flag__"
    },
    "methods": [
      "GET"
    ],
    "patterns": [
      "_data_stream/_stats",
      "_data_stream/{name}/_stats"
    ],
    "documentation": "https://www.elastic.co/guide/en/elasticsearch/reference/master/data-streams.html"
  }
}
