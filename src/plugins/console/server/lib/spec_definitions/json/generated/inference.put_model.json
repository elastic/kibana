{
  "inference.put_model": {
    "url_params": {
      "error_trace": "__flag__",
      "filter_path": [],
      "human": "__flag__",
      "pretty": "__flag__"
    },
    "url_components": {
      "task_type": [
        "sparse_embedding",
        "text_embedding",
        "rerank",
        "completion"
      ]
    },
    "methods": [
      "PUT"
    ],
    "patterns": [
      "_inference/{inference_id}",
      "_inference/{task_type}/{inference_id}"
    ],
    "documentation": "https://www.elastic.co/guide/en/elasticsearch/reference/master/put-inference-api.html",
    "availability": {
      "stack": true,
      "serverless": true
    }
  }
}
