{
  "cluster.health": {
    "url_params": {
      "expand_wildcards": [
        "open",
        "closed",
        "hidden",
        "none",
        "all"
      ],
      "level": [
        "cluster",
        "indices",
        "shards"
      ],
      "local": "__flag__",
      "master_timeout": "",
      "timeout": "",
      "wait_for_active_shards": "",
      "wait_for_nodes": "",
      "wait_for_events": [
        "immediate",
        "urgent",
        "high",
        "normal",
        "low",
        "languid"
      ],
      "wait_for_no_relocating_shards": "__flag__",
      "wait_for_no_initializing_shards": "__flag__",
      "wait_for_status": [
        "green",
        "yellow",
        "red"
      ]
    },
    "methods": [
      "GET"
    ],
    "patterns": [
      "_cluster/health",
      "_cluster/health/{indices}"
    ],
    "documentation": "https://www.elastic.co/guide/en/elasticsearch/reference/master/cluster-health.html"
  }
}
