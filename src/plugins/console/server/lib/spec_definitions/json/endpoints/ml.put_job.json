      "allow_no_indices": "__flag__",
      "ignore_throttled": "__flag__",
      "expand_wildcards": [
        "open",
        "closed",
        "hidden",
        "none",
        "all"
      ]
    },
    "methods": [
      "PUT"
    ],
    "patterns": [
      "_ml/anomaly_detectors/{job_id}"
    ],
    "documentation": "https://www.elastic.co/guide/en/elasticsearch/reference/current/ml-put-job.html"
  }
}
