{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "config": {
    "kibana": { "type": "map", "mapStyle": "default", "latitude": 25, "longitude": -70, "zoom": 3}
  },
  "width": 512,
  "height": 512,
  "marks": [
    {
      "type": "rect",
      "encode": {
        "enter": {
          "fill": {"value": "#0f0"},
          "width": {"signal": "width"},
          "height": {"signal": "height"}
        }
      }
    }
  ]
}
