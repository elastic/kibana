{
  "id": "discover",
  "version": "kibana",
  "server": true,
  "ui": true,
  "requiredPlugins": [
    "charts",
    "data",
    "embeddable",
    "inspector",
    "fieldFormats",
    "kibanaLegacy",
    "urlForwarding",
    "navigation",
    "uiActions",
    "savedObjects",
    "indexPatternFieldEditor"
  ],
  "optionalPlugins": [
    "alerting",
    "home",
    "share",
    "usageCollection",
    "spaces",
    "triggersActionsUi"
  ],
  "requiredBundles": ["kibanaUtils", "home", "kibanaReact", "dataViews"],
  "extraPublicDirs": ["common"],
  "owner": {
    "name": "Data Discovery",
    "githubTeam": "kibana-data-discovery"
  },
  "description": "This plugin contains the Discover application and the saved search embeddable."
}
