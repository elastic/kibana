{
  "id": "discover",
  "version": "kibana",
  "server": true,
  "ui": true,
  "requiredPlugins": [
    "charts",
    "data",
    "embeddable",
    "inspector",
    "fieldFormats",
    "urlForwarding",
    "navigation",
    "uiActions",
    "savedObjects",
    "savedObjectsFinder",
    "savedObjectsManagement",
    "dataViewFieldEditor",
    "dataViewEditor",
    "expressions",
    "unifiedFieldList",
    "unifiedSearch",
    "unifiedHistogram"
  ],
  "optionalPlugins": [
    "home",
    "share",
    "usageCollection",
    "spaces",
    "triggersActionsUi",
    "savedObjectsTaggingOss"
  ],
  "requiredBundles": ["kibanaUtils", "kibanaReact", "dataViews", "unifiedSearch", "savedSearch"],
  "extraPublicDirs": ["common"],
  "owner": {
    "name": "Data Discovery",
    "githubTeam": "kibana-data-discovery"
  },
  "description": "This plugin contains the Discover application and the saved search embeddable."
}
