{
  "id": "discover",
  "version": "kibana",
  "server": true,
  "ui": true,
  "requiredPlugins": [
    "charts",
    "data",
    "embeddable",
    "inspector",
    "fieldFormats",
    "urlForwarding",
    "navigation",
    "uiActions",
    "savedObjects",
    "dataViewFieldEditor"
  ],
  "optionalPlugins": ["home", "share", "usageCollection", "spaces"],
  "requiredBundles": ["kibanaUtils", "home", "kibanaReact", "dataViews"],
  "extraPublicDirs": ["common"],
  "owner": {
    "name": "Data Discovery",
    "githubTeam": "kibana-data-discovery"
  },
  "description": "This plugin contains the Discover application and the saved search embeddable."
}
