  "ui": true,
  "requiredPlugins": ["expressions", "fieldFormats", "charts", "visualizations", "presentationUtil", "data"],
  "requiredBundles": ["kibanaUtils", "kibanaReact"],
  "optionalPlugins": ["usageCollection"],
  "extraPublicDirs": ["common"]
}
