.row-fluid
  .row-fluid
    .span3
      %h5 Length
      %input.input-mini{"ng-model" => "panel.size", :type => "number"}/
    .span3
      %h6 Font Size
      %select.input-small{"ng-model" => "panel.style['font-size']", "ng-options" => "f for f in ['6pt','7pt','8pt','9pt','10pt','12pt','14pt','16pt','18pt','20pt','24pt','28pt','32pt','36pt','42pt','48pt','52pt','60pt','72pt']"}
