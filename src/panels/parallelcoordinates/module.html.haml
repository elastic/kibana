:css
  svg {
      font-size: 14px;
  }
  
  .foregroundlines {
      fill: none;
      stroke-opacity: 0.3;
      stroke-width: 1.5px;
  }
  
  .foregroundlines.fade {
      display:none
  }
  
  .legend line {
      stroke-width: 2px;
  }
  
  .brush .extent {
      fill-opacity: .3;
      stroke: #fff;
      shape-rendering: crispEdges;
  }
  
  .axis line, .axis path {
      fill: none;
      stroke: #000;
      shape-rendering: crispEdges;
  }
  
  .axis text {
      text-shadow: 0 1px 0 #fff;
      cursor: move;
  }
%kibana-panel{"ng-controller" => "parallelcoordinates", "ng-init" => "init()"}
  %span.panelextra.pointer{"ng-show" => "panel.spyable", :style => "position:absolute;right:0px;top:0px"}
    %i.icon-eye-open{"bs-modal" => "'partials/modal.html'"}
  %div{:parallelcoordinates => "", :params => "{{panel}}", :style => "height:{{panel.height || row.height}}"}
