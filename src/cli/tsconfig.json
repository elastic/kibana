{
  "extends": "@kbn/tsconfig-base/tsconfig.json",
  "compilerOptions": {
    "outDir": "target/types"
  },
  "include": [
    "encryption_keys/**/*",
    "health_gateway/**/*",
    "kibana/**/*",
    "keystore/**/*",
    "plugin/**/*",
    "profiler/**/*",
    "serve/**/*",
    "setup/**/*",
    "verification_code/**/*",
    "*.ts"
  ],
  "kbn_references": [
    "@kbn/core",
    "@kbn/setup-node-env",
    "@kbn/interactive-setup-plugin",
    "@kbn/health-gateway-server",
    "@kbn/core-elasticsearch-client-server-internal",
    "@kbn/core-elasticsearch-server-internal",
    "@kbn/utils",
    "@kbn/repo-info",
    "@kbn/safer-lodash-set",
    "@kbn/config",
    "@kbn/dev-utils",
    "@kbn/projects-solutions-groups",
    "@kbn/apm-config-loader",
    "@kbn/telemetry",
    "@kbn/cli-dev-mode",
    "@kbn/import-resolver",
    "@kbn/optimizer",
    "@kbn/picomatcher",
    "@kbn/repo-source-classifier",
    "@kbn/vite-config",
    "@kbn/vite-optimizer",
    "@kbn/vite-server"
  ],
  "exclude": ["target/**/*", "**/*.test.js", "**/*.test.ts"]
}
