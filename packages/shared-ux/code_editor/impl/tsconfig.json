{
  "extends": "../../../../tsconfig.base.json",
  "parserOptions": "extraFileExtensions",
  "compilerOptions": {
    "outDir": "target/types",
    "types": [
      "jest",
      "node",
      "react",
      "@emotion/react/types/css-prop",
      "@kbn/ambient-ui-types",

    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.mdx",
  ],
  "exclude": [
    "target/**/*"
  ],
  "kbn_references": [
    "@kbn/monaco",
    "@kbn/i18n",
    "@kbn/i18n-react",
    "@kbn/code-editor-mocks",
  ]
}
