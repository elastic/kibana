{
  "extends": "../../../../tsconfig.bazel.json",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "outDir": "target_types",
    "stripInternal": false,
    "types": [
      "jest",
      "node",
      "@kbn/ambient-ui-types",
      "@emotion/react/types/css-prop"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts*",
    "**/languages/*d.ts*",
    "**/languages/**/*.d.ts*",
  ]
}
