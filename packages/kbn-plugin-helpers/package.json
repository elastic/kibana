{
  "name": "@kbn/plugin-helpers",
  "version": "1.0.0",
  "private": true,
  "description": "Just some helpers for kibana plugin devs.",
  "license": "Apache-2.0",
  "kibana": {
    "devOnly": true
  },
  "main": "target/index.js",
