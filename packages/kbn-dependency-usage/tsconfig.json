{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "target/types",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
  },
  "include": ["**/*.ts"],
  "exclude": [
    "target/**/*"
  ],
  "kbn_references": [
    "@kbn/repo-info",
  ],
}
