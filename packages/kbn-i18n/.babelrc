{
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ],
  "presets": [
    "@babel/preset-react",
    "@babel/typescript"
  ],
  "env": {
    "web": {
      "presets": [
        ["@babel/env", {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 5%",
              "Safari 7"
            ]
          }
        }]
      ]
    },
    "node": {
      "presets": [
        ["@babel/env", {
          "targets": {
            "node": "8.11.4"
          }
        }]
      ]
    }
  },
  "ignore": [
    "**/*.test.ts"
  ]
}
