{
  "extends": "../../tsconfig.bazel.json",
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "incremental": false,
    "outDir": "target_types",
    "rootDir": "src",
    "skipLibCheck": true,
    "target": "es2015",
    "types": [
      "node",
      "@storybook/core-common",
      "@kbn/ambient-storybook-types"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
