{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "target_types",
    "skipLibCheck": true,
    "target": "es2015",
    "types": [
      "node",
      "@kbn/ambient-storybook-types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "kbn_references": [
    "@kbn/dev-utils",
    "@kbn/ui-shared-deps-npm",
    "@kbn/ui-shared-deps-src",
    "@kbn/utils",
    "@kbn/ambient-storybook-types"
  ]
}
