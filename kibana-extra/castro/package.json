{
  "name": "castro",
  "version": "0.0.1",
  "description": "castro",
  "main": "index.js",
  "private": true,
  "license": "Elastic-License",
  "kibana": {
    "version": "kibana",
    "templateVersion": "1.0.0"
  },
  "scripts": {
    "_preinstall": "node ../../kibana/preinstall_check",
    "kbn": "node ../../kibana/scripts/kbn",
    "tslint": "tslint --project tsconfig.json --format stylish",
    "start": "plugin-helpers start --config ../config/kibana/kibana.yml",
    "test:server": "plugin-helpers test:server",
    "test:browser": "plugin-helpers test:browser",
    "precommit": "lint-staged",
    "build": "plugin-helpers build",
    "start-deps": "node ../../kibana/scripts/es.js snapshot -E path.data=../../data",
    "debug": " node --inspect ../../kibana/scripts/kibana.js --config ../../config/kibana/kibana.yml  --env.name=development --optimize.enabled=false --plugin-path=."
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn run tslint"
  },
  "devDependencies": {
    "@elastic/eui": "^3.0.0",
    "@kbn/plugin-helpers": "link:../../kibana/packages/kbn-plugin-helpers",
    "@types/boom": "^7.2.0",
    "@types/elasticsearch": "^5.0.24",
    "@types/get-port": "^3.2.0 ",
    "@types/hapi": "^15.0.0",
    "@types/nodegit": "^0.22.1",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/redux-actions": "^2.3.0",
    "@types/file-type": "^5.2.1",
    "@types/git-url-parse": "^9.0.0",
    "expect.js": "^0.3.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {

    "boom": "^7.2.0",
    "file-type": "^8.1.0",
    "get-port": "^3.2.0",
    "git-url-parse": "^9.0.1",
    "highlights": "^3.1.1",
    "javascript-typescript-langserver": "^2.10.0",
    "moment": "^2.20.1",
    "monaco-editor": "^0.13.1",
    "nodegit": "^0.22.1",
    "popper.js": "^1.14.3",
    "prop-types": "15.5.8",
    "react": "^16.3.0",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-saga": "^0.16.0",
    "rimraf": "^2.6.2",
    "vscode-jsonrpc": "^3.6.2",
    "vscode-languageserver": "^4.2.1"
  }
}
