                        "subtechnique": [
                            {
                                "id": "T1016.001",
                                "name": "Internet Connection Discovery",
                                "reference": "https://attack.mitre.org/techniques/T1016/001/"
                            }
                        ]
                    },
                    {
                        "id": "T1057",
                        "name": "Process Discovery",
                        "reference": "https://attack.mitre.org/techniques/T1057/"
                    }
                ]
