{
	"root": true,

	"extends": "@ljharb",

	"rules": {
		"complexity": [2, 18],
		"consistent-return": [1],
		"max-params": [2, 8],
		"max-statements": [2, 32],
		"no-extra-parens": [1],
		"no-continue": [1],
		"no-magic-numbers": 0,
		"no-restricted-syntax": [2, "BreakStatement", "DebuggerStatement", "ForInStatement", "LabeledStatement", "WithStatement"],
		"operator-linebreak": 1
	}
}
