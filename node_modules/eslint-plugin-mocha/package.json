{
  "name": "eslint-plugin-mocha",
  "version": "1.1.0",
  "description": "Eslint rules for mocha.",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "npm run test:unit --coverage && npm run check-coverage",
